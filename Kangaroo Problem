#include<stdio.h>
int main()
{
  int x1,v1,x2,v2;
    float p1;
    int p2;
  scanf("%d %d %d %d",&x1,&v1,&x2,&v2);
    if((v1-v2)!=0)
    {
    p2=(x2-x1)%(v1-v2);
    p1=(x2-x1)/(v1-v2);
    if(p2==0 && p1>0)
       printf("YES");
    else
        printf("NO");
    }
    if(v1==v2 && x1!=x2)
        printf("NO");
}
